<!-- ************ Fire section *********** -->

 <animation>
  <type>material</type>
  <object-name>fire_l-wing</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.2</blue>
   <factor-prop>controls/lighting/overhead_instr</factor-prop>
  </emission>
 </animation>

<!-- Main switch + lever lock -->
<model>
 <path>sw_fire_main.xml</path>
  <offsets>
    <x-m>-0.009</x-m>
    <y-m>0.2671</y-m>
    <z-m>-0.0395</z-m>
  </offsets>
</model>

 <animation>
   <type>rotate</type>
   <object-name>lever-lock</object-name>
   <property>an24/FireFeather/main</property>
    <factor>90</factor>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.2671</y-m>
    <z-m>-0.0395</z-m>
  </center>
  <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
 </animation> 

<!-- Left engine fire extinguishers -->
<model>
 <path>btn01_exting_engineL.xml</path>
  <offsets>
    <x-m>-0.0136</x-m>
    <y-m>0.0467</y-m>
    <z-m>-0.054</z-m>
    <pitch-deg>90</pitch-deg>
  </offsets>
</model>

<animation>
  <type>pick</type>
  <object-name>cover01</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        if( getprop("an24/FireFeather/cover_btn01" ) == 1.0 ){
        interpolate("an24/FireFeather/cover_btn01", 0.0, 0.2 );
        }
        else {
        interpolate("an24/FireFeather/cover_btn01", 1.0, 0.2 );
        }
      </script>
    </binding>
  </action>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>cover01</object-name>
   <property>an24/FireFeather/cover_btn01</property>
   <factor>80</factor>
  <center>
    <x-m>0.0047</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.0694</z-m>
  </center>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
 </animation>

