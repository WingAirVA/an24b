<?xml version="1.0"?>

<!-- Knob to select group -->

<PropertyList>
 <path>../../shared/knobs_buttons/11p1n.ac</path>

 <animation>
   <type>knob</type>
   <object-name>11p1n</object-name>
   <property>an24/FireTest/selection</property>
   <factor>30</factor>
   <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
   </axis>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>an24/FireTest/selection</property>
      <factor>-1</factor>
      <min>0</min>
      <max>12</max>                
      <wrap>true</wrap>
     </binding>
    </action>
     <binding>
      <command>property-assign</command>
      <property>an24/FireFeather/mainresetdone</property>
      <value>0</value>
     </binding>
 </animation>

</PropertyList>
