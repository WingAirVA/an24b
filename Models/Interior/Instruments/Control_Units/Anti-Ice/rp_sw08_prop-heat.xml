<?xml version="1.0"?>

<!-- Prop anti-ice ON -->

<PropertyList>
 <path>../../../shared/switches/good-for-all.ac</path>
 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Anti-Ice/prop-heat", 1.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>propice_on</tooltip-id>
      <label>Propeller Anti-Ice
  Normal System</label>
    </binding>
   </hovered>
</animation>

<animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Anti-Ice/prop-heat", 0.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>propice_off</tooltip-id>
      <label> Propeller Anti-Ice
Emergency System</label>
    </binding>
   </hovered>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/Anti-Ice/prop-heat</property>
  <interpolation>
    <entry><ind>0.0</ind>	<dep>-20.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>20.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>
 

</PropertyList>
