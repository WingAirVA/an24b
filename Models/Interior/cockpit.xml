<!--Front seats-->

<model>
  <name>Seats</name>
  <path>Aircraft/an24b/Models/Interior/seats.ac</path>
  <offsets>
    <x-m>2.78</x-m>
    <y-m>0.0</y-m>
    <z-m>0.3777</z-m>
  </offsets>
</model>

<animation>
  <type>pick</type>
  <object-name>armrest</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        if( getprop("an24/animations/armrest" ) == 1.0 ){
        interpolate("an24/animations/armrest", 0.0, 1.0 );
        }
        else {
        interpolate("an24/animations/armrest", 1.0, 1.0 );
        }
      </script>
    </binding>
  </action>
</animation>

<animation>
  <type>rotate</type>
  <object-name>armrest</object-name>
  <property>an24/animations/armrest</property>
  <factor>90.0</factor>
  <center>
    <x-m>0.1817</x-m>
    <y-m>0.0</y-m>
    <z-m>0.3076</z-m>    
  </center>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>    
  </axis>
</animation>

<!-- Navigator's center seat-->
<animation>
  <type>pick</type>
  <object-name>centerseatbottom</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        if( getprop("an24/animations/centerseat" ) == 1.0 ){
        interpolate("an24/animations/centerseat", 0.0, 3.0 );
        }
        else {
        interpolate("an24/animations/centerseat", 1.0, 3.0 );
        }
      </script>
    </binding>
  </action>
</animation>

<animation>
  <type>rotate</type>
  <object-name>centerseatbottom</object-name>
  <property>an24/animations/centerseat</property>
  <factor>183.0</factor>
  <center>
    <x-m>0.3027</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.2587</z-m>    
  </center>
  <axis>
    <x>0</x>
    <y>-1</y>
    <z>0</z>    
  </axis>
</animation>

<animation>
