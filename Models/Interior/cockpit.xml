<?xml version="1.0"?>

<PropertyList>
 <path>cockpit.ac</path>

  <model>
   <path>Aircraft/Generic/marker.xml</path>
  </model>

<!--Cockpit animations-->

 <effect>
  <inherits-from>Aircraft/an24b/Models/Effects/alu</inherits-from>
   <object-name>hull</object-name>
   <object-name>lining</object-name>
   <object-name>fabric</object-name>
   <object-name>speaker_panel</object-name>
   <object-name>c-frameL</object-name>
   <object-name>c-frameR</object-name>
   <object-name>topwinframe</object-name>
   <object-name>railer</object-name>
   <object-name>screws</object-name>
 </effect>
 <effect>
  <inherits-from>Effects/model-transparent</inherits-from>
  <object-name>screenfoil</object-name>
  <object-name>foil</object-name>
 </effect>

 <animation>
   <type>material</type>
   <object-name>mp_lightplate</object-name>
   <emission>
     <red>1.0</red>
     <green>0.4</green>
     <blue>0.3</blue>
     <factor-prop>controls/lighting/panel-m_red</factor-prop>
   </emission>
 </animation>

 <animation>
   <type>slider</type>
   <object-name>gardinenstange</object-name>
   <visible>true</visible>
  <drag-direction>vertical</drag-direction>
   <action>
     <binding>
       <command>property-adjust</command>
       <property>an24/animations/gardine</property>
       <factor>0.08</factor>
       <min>0</min>
       <max>1.0</max>
       <wrap>false</wrap>
     </binding>
   </action>
 </animation>

<animation>
  <type>scale</type>
  <object-name>gardine</object-name>
  <property>an24/animations/gardine</property>
   <center>
   <x-m>2.055</x-m>
   <y-m>0.0</y-m>
   <z-m>1.3317</z-m>
  </center>
  <x-min>1.0</x-min>
  <y-min>1.0</y-min>
  <z-min>0.1</z-min>
  <x-factor>1.0</x-factor>
  <y-factor>1.0</y-factor>
  <z-factor>8.0</z-factor>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>gardinenstange</object-name>
  <property>an24/animations/gardine</property>
  <factor>200.0</factor>
  <offset-deg>25</offset-deg>
  <center>
    <x-m>2.0468</x-m>
    <y-m>-0.4788</y-m>
    <z-m>1.3319</z-m>
  </center>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>    
  </axis>
 </animation>

<!--Yoke-->

<animation>
  <type>rotate</type>
  <object-name>yokeL</object-name>
  <object-name>radio_base_l</object-name>
  <object-name>radio_press_me_l</object-name>
  <object-name>spu_base_l</object-name>
  <object-name>spu_press_me_l</object-name>
  <property>controls/flight/aileron</property>
  <factor>90.0</factor>
  <offset-deg>0.0</offset-deg>
  <center>
    <x-m>0.0</x-m>
    <y-m>-0.4738</y-m>
    <z-m>0.8278</z-m>
  </center>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>    
  </axis>
</animation>

<animation>
  <type>translate</type>
  <object-name>yokeL</object-name>
  <object-name>radio_base_l</object-name>
  <object-name>radio_press_me_l</object-name>
  <object-name>spu_base_l</object-name>
  <object-name>spu_press_me_l</object-name>
  <property>controls/flight/elevator</property>
  <factor>0.08</factor>
  <offset-deg>0.0</offset-deg>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>    
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>yokeR</object-name>
  <object-name>radio_base_r</object-name>
  <object-name>radio_press_me_r</object-name>
  <object-name>spu_base_r</object-name>
  <object-name>spu_press_me_r</object-name>
  <property>controls/flight/aileron</property>
  <factor>90.0</factor>
  <offset-deg>0.0</offset-deg>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.4738</y-m>
    <z-m>0.8278</z-m>
  </center>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>    
  </axis>
</animation>

<animation>
  <type>translate</type>
  <object-name>yokeR</object-name>
  <object-name>radio_base_r</object-name>
  <object-name>radio_press_me_r</object-name>
  <object-name>spu_base_r</object-name>
  <object-name>spu_press_me_r</object-name>
  <property>controls/flight/elevator</property>
  <factor>0.08</factor>
  <offset-deg>0.0</offset-deg>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>    
  </axis>
</animation>

